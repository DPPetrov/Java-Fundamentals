package com.company;
public class Main {

    public static void main(String[] args) {
        int[] numbers = new int[10]; // създай ми масив с памет за 10 елементс
        numbers[0] = 10; //елемтнът на позиция 0 има стойност 10;
        numbers[1] = 20;
        numbers[2] = 30;
        //System.out.println(numbers[2]); // отпечатва отделен елемент
        System.out.println(numbers.length); // отпечатваме дължината на масива - колко елемента има в масива
        //отпечатва колкото елемента има в масива

    }
}
