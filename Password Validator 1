import java.util.Scanner;

public class PasswordValidator {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        //Създаваме инпут за паролата
        //Създаваме булеви променливи, дали паролата отговаря на условието
        //Правим проверка, дали паролата отговаря на условието и отпечатва какво недостига
        //Създаваме метод, който отпечатва, че паролата отговаря на условието

        String input = sc.nextLine();

        passwordLength(input);
    }

    private static  boolean passwordLength(String password){
        return (password.length() >= 6 && password.length() <= 10); // това ще върне true/false, в зависимост дали
        //отговаря на условието
    }

    private static boolean isContentCorrect (String password) {
        for (char symbol : password.toCharArray()) {
            if (!Character.isLetterOrDigit(symbol)){
                return false;
            }

        }

        return true;
    }
    
    public static boolean isDigitCount (String password){
        int digitCount = 0;

        for (char symbol: password.toCharArray()) {
            if (Character.isDigit(symbol)){
                digitCount++;
            }
        }
        
        if (digitCount >= 2){
            return true;
        }else{
            return false;
        }
    }
}
